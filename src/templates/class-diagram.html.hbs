{{/if}}
{{/each}}

{{#each entities}}
  class {{{name}}}{
{{#valueProperties}}
    +{{type}}{{#if isArray}}[]{{/if}}: {{name}}
{{/valueProperties}}
{{#referenceProperties}}
    +{{referenceTypeName}}{{#if isArray}}[]{{/if}}: {{name}};
{{/referenceProperties}}
  }
{{/each}}
    </div>
