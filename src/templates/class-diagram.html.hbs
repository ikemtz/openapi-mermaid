    <script>mermaid.initialize({startOnLoad:true});</script>
  </head>
  <body>
    <div class="mermaid">
classDiagram

{{#each entities}}
{{#if importTypes}}
  {{#importTypes}}
  {{{../this.name}}} ..> {{name}}
