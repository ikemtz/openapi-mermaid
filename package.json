{
  "name": "openapi-mermaid",
  "version": "1.0.0",
  "description": "Creates Mermaid diagrams using OpenApi v3 docs",
  "main": "index.js",
  "scripts": {
    "build": "npm run format && npm run lint && tsc && cp -rfv ./src/templates ./lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "test": "npm run build && jest --collect-coverage",
    "prep:lib": "rm -rv ./lib/*.spec.* && rm -rv ./lib/**/*.spec.* && cp -v ./package.json ./lib/ && cp -v ./LICENSE ./lib/ && cp -v ./README.md ./lib/",
    "debug": "npm run build && node --inspect-brk ./lib/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikemtz/openapi-mermaid.git"
  },
  "author": "@IkeMtz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikemtz/openapi-mermaid/issues"
  },
  "keywords": [
    "OpenAPI",
    "Mermaid",
    "Class",
    "Diagrams",
    "Swagger"
  ],
  "homepage": "https://github.com/ikemtz/openapi-mermaid#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.9.0",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.3",
    "jest-junit": "^12.1.0",
    "openapi3-ts": "^2.0.1",
    "prettier": "^2.3.0",
    "rxjs": "^7.1.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "node-fetch": "^2.6.1"
  }
}
